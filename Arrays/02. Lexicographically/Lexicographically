function solve(args){
    var firstChar = args[0]
    .split('\n'),
        secondChar = args[1]
        .split('\n');
      
    var char1 = firstChar.sort();
    var char2 = secondChar.sort();
    
    if (char1 < char2) {
        console.log('<');
    } else if (char1 > char2) {
        console.log('>');
    } else {
        console.log('=');
    }

}
